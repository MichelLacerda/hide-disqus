{
    "manifest_version": 3,
    "name": "Disable Discus",
    "description": "Disable Disqus in Websites",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [{
        "matches": ["https://*/*"],
        "js": ["./content.js"]
    }],
    "permissions": ["scripting", "activeTab", "storage"]
}